programa {
    
    funcao inicio() {
        real numero1, numero2
        real media

        escreva("Entre com um número: ")
        leia(numero1)

        escreva("Entre com outro número: ")
        leia(numero2)

        media = (numero1 + numero2) / 2.0
        
        escreva("Média: ", media)
    }
}
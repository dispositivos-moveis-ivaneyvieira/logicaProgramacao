Programa calculaMedia
    início
        Declare numero1, numero2: real;
        Declare media: real;

        Escreva 'Entre com um número: ';
        Leia numero1;

        Escreva 'Entre com outro número: ';
        Leia numero2;

        media <- (numero1 + numero2) / 2;
        
        Escreva 'Média: ', media;
    fim.